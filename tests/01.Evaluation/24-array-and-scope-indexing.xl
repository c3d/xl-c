A -> "Outer A"
C -> "Outer C"

foo ->
    A -> "Inner A"
    B -> "Inner B"

    "Outer A" -> "Indexed by Outer A"

writeln "foo.A=", foo.A
writeln "foo.B=", foo.B
writeln "foo.C=", foo.C

writeln "foo[A]=", foo[A]
writeln "foo[B]=", foo[B]
writeln "foo[C]=", foo[C]

writeln "foo(A)=", foo(A)
writeln "foo(B)=", foo(B)
writeln "foo(C)=", foo(C)

writeln "foo A=", foo A
writeln "foo B=", foo B
writeln "foo C=", foo C

    
